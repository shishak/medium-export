---
layout: layouts/base.njk
---


<div id="columns">
{% for item in medium.post %}
  <figure>
  <figcaption>
    <p class="meta">{{ item.pubDate | date }}</p>
    <h3><a href="/post/{{ item.path }}">{{ item.title }}</a></h3>
    {{ item.description | safe }}
   </figcaption>
  </figure>
 {% endfor %}
</div>
